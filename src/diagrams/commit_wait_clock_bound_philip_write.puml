@startuml
!include ../style.puml

participant Philip as p
participant Green as s1

p -> s1 :put("title", "After Dawn")
note over s1
 t=4
 error=+-1
 earliest=3, 
 latest=5
end note 
s1 -> s1:waitUntilTimeInPast(5)
note over s1
 t=6
 error=+-1
 earliest=5, 
 latest=7
 title@5="After Dawn"
end note 
p <-- s1
@enduml
